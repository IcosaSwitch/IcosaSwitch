{
  "name": "IcosaSwitch",
  "version": "1.3.0",
  "productName": "IcosaSwitch",
  "description": "Tools for Switch Hacking",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "dist:win": "electron-builder --win",
    "dist:linux": "electron-builder --linux",
    "dist:darwin": "electron-builder --mac"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pharuxtan/IcosaSwitch.git"
  },
  "author": "Pharuxtan",
  "license": "MIT",
  "dependencies": {
    "cross-spawn": "^7.0.1",
    "decompress-zip": "^0.2.2",
    "ejs": "^2.6.1",
    "ejs-electron": "^2.0.3",
    "electron-updater": "^4.0.6",
    "fs": "0.0.1-security",
    "https-proxy-agent": "^3.0.1",
    "jquery": "^3.4.0",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "platform-folders": "^0.4.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "electron": "^7.0.0",
    "electron-builder": "^21.2.0"
  },
  "build": {
    "appId": "icosaswitch",
    "productName": "IcosaSwitch",
    "artifactName": "${productName}-${version}.${ext}",
    "copyright": "Copyright Â© 2019 IcosaSwitch",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "win": {
      "target": [
        {
          "target": "nsis"
        }
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico",
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.utilities",
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "maintainer": "Pharuxtan",
      "vendor": "IcosaSwitch",
      "synopsis": "IcosaSwitch",
      "description": "Tools for Switch Hacking",
      "category": "Utility"
    },
    "files": [
      "!{dist,dev-app-update.yml}"
    ],
    "compression": "maximum",
    "asar": true
  },
  "bugs": {
    "url": "https://github.com/Pharuxtan/IcosaSwitch/issues"
  },
  "homepage": "https://github.com/Pharuxtan/IcosaSwitch#readme"
}
