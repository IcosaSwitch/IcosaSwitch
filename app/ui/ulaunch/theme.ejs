<div style="position:relative;" width="360" height="310">
  <div style="position:fixed;left:<%- over %>; overflow: hidden;"></div>
  <div style="<%- font %>;position:absolute;left:<%- field %>;" width="360" height="310">
    <input class="bgtheme" type="button" disabled/>
    <img width="320" height="180" style="position:absolute;top: 20; left: 20" alt="Background" src="<%- theme.preview.background %>">
    <img width="300" height="21.25" style="position: absolute;top: <%- (mainmenu["top_menu_bg"]["y"] / 4 + 20) %>; left: <%- (mainmenu["top_menu_bg"]["x"] / 4 + 20) %>;visibility: <%- (mainmenu["top_menu_bg"]["visible"]) ? "visible" : "hidden" %>;" alt="TopMenu" src="<%- theme.preview.topmenu %>">
    <input style="position: absolute;top: <%- (mainmenu["time_text"]["y"] / 4 + 20) %>; left: <%- (mainmenu["time_text"]["x"] / 4 + 20) %>;border: 0;font-family: 'Custom'; font-size: 5; font-weight: bold; background-color: transparent; border: 0; color: <%- textcolor %>;visibility: <%- (mainmenu["time_text"]["visible"]) ? "visible" : "hidden" %>;" alt="Time" type="text" value="20:06" size="1" disabled>
    <input style="position: absolute;top: <%- (mainmenu["battery_text"]["y"] / 4 + 20) %>; left: <%- (mainmenu["battery_text"]["x"] / 4 + 20) %>;border: 0;font-family: 'Custom'; font-size: 5; font-weight: bold; background-color: transparent; border: 0; color: <%- textcolor %>;visibility: <%- (mainmenu["battery_text"]["visible"]) ? "visible" : "hidden" %>;" alt="Perc" type="text" value="99%" size="1" disabled>
    <input style="position: absolute;top: <%- (mainmenu["firmware_text"]["y"] / 4 + 20) %>; left: <%- (mainmenu["firmware_text"]["x"] / 4 + 20) %>;border: 0;font-family: 'Custom'; font-size: 5; font-weight: bold; background-color: transparent; border: 0; color: <%- textcolor %>;visibility: <%- (mainmenu["firmware_text"]["visible"]) ? "visible" : "hidden" %>;" alt="FirmVer" type="text" value="9.0.1" size="1" disabled>
    <img width="320" height="33.75" style="position: absolute;top: <%- (mainmenu["banner_image"]["y"] / 4 + 20) %>; left: <%- (mainmenu["banner_image"]["x"] / 4 + 20) %>;visibility: <%- (mainmenu["banner_image"]["visible"]) ? "visible" : "hidden" %>;" alt="BannerInstalled" src="<%- theme.preview.bannerinstalled %>">
    <img width="15" height="15" style="position: absolute;top: <%- (mainmenu["logo_icon"]["y"] / 4 + 20) %>; left: <%- (mainmenu["logo_icon"]["x"] / 4 + 20) %>;visibility: <%- (mainmenu["logo_icon"]["visible"]) ? "visible" : "hidden" %>;" alt="Logo" src="https://github.com/XorTroll/uLaunch/blob/master/LibraryAppletQMenu/RomFs/Logo.png?raw=true">
    <img width="12.5" height="12.5" style="position: absolute;top: <%- (mainmenu["connection_icon"]["y"] / 4 + 20) %>; left: <%- (mainmenu["connection_icon"]["x"] / 4 + 20) %>;visibility: <%- (mainmenu["connection_icon"]["visible"]) ? "visible" : "hidden" %>;" alt="NoConnectionIcon" src="<%- theme.preview.noconnectionicon %>">
    <input style="position: absolute;top: <%- (mainmenu["user_icon"]["y"] / 4 + 20) %>; left: <%- (mainmenu["user_icon"]["x"] / 4 + 20) %>; width: 12.5; height: 12.5; border: 0; background-color: #808080;visibility: <%- (mainmenu["user_icon"]["visible"]) ? "visible" : "hidden" %>;" alt="User" type="button">
    <img width="12.5" height="12.5" style="position: absolute;top: <%- (mainmenu["web_icon"]["y"] / 4 + 20) %>; left: <%- (mainmenu["web_icon"]["x"] / 4 + 20) %>;visibility: <%- (mainmenu["web_icon"]["visible"]) ? "visible" : "hidden" %>;" alt="WebIcon" src="<%- theme.preview.webicon %>">
    <img width="7.5" height="7.5" style="position: absolute;top: <%- (mainmenu["battery_icon"]["y"] / 4 + 20) %>; left: <%- (mainmenu["battery_icon"]["x"] / 4 + 20) %>;visibility: <%- (mainmenu["battery_icon"]["visible"]) ? "visible" : "hidden" %>;" alt="BatteryNormalIcon" src="<%- theme.preview.batterynormalicon %>">
    <img width="12.5" height="12.5" style="position: absolute;top: <%- (mainmenu["settings_icon"]["y"] / 4 + 20) %>; left: <%- (mainmenu["settings_icon"]["x"] / 4 + 20) %>;visibility: <%- (mainmenu["settings_icon"]["visible"]) ? "visible" : "hidden" %>;" alt="SettingsIcon" src="<%- theme.preview.settingsicon %>">
    <img width="12.5" height="12.5" style="position: absolute;top: <%- (mainmenu["themes_icon"]["y"] / 4 + 20) %>; left: <%- (mainmenu["themes_icon"]["x"] / 4 + 20) %>;visibility: <%- (mainmenu["themes_icon"]["visible"]) ? "visible" : "hidden" %>;" alt="ThemesIcon" src="<%- theme.preview.themesicon %>">
    <img width="60" height="17.5" style="position: absolute;top: <%- (mainmenu["menu_toggle_button"]["y"] / 4 + 20) %>; left: <%- (mainmenu["menu_toggle_button"]["x"] / 4 + 20) %>;visibility: <%- (mainmenu["menu_toggle_button"]["visible"]) ? "visible" : "hidden" %>;" alt="ToggleClick" src="<%- theme.preview.toggleclick %>">
    <input style="position: absolute;top: <%- (mainmenu["banner_name_text"]["y"] / 4 + 20) %>; left: <%- (mainmenu["banner_name_text"]["x"] / 4 + 20) %>; border: 0;font-family: 'Custom'; font-size: 7.5; font-weight: bold; background-color: transparent; border: 0; color: <%- textcolor %>;visibility: <%- (mainmenu["banner_name_text"]["visible"]) ? "visible" : "hidden" %>;" alt="itemName" type="text" value="Show all titles" size="119.5" disabled>
    <input style="position: absolute;top: <%- (mainmenu["banner_author_text"]["y"] / 4 + 20) %>; left: <%- (mainmenu["banner_author_text"]["x"] / 4 + 20) %>; border: 0;font-family: 'Custom'; font-size: 5; font-weight: bold; background-color: transparent; border: 0; color: <%- textcolor %>;visibility: <%- (mainmenu["banner_author_text"]["visible"]) ? "visible" : "hidden" %>;" alt="itemAuthor" type="text" value="" size="119.5" disabled>
    <input style="position: absolute;top: <%- (mainmenu["banner_version_text"]["y"] / 4 + 20) %>; left: <%- (mainmenu["banner_version_text"]["x"] / 4 + 20) %>; border: 0;font-family: 'Custom'; font-size: 5; font-weight: bold; background-color: transparent; border: 0; color: <%- textcolor %>;visibility: <%- (mainmenu["banner_version_text"]["visible"]) ? "visible" : "hidden" %>;" alt="itemVersion" type="text" value="" size="119.5" disabled>
    <img width="64" height="64" style="position: absolute;top: <%- (mainmenu["items_menu"]["y"] / 4 + 20) %>; left: 44.5" alt="AllTitles" src="<%- theme.preview.alltitles %>">
    <img width="64" height="64" style="position: absolute;top: <%- (mainmenu["items_menu"]["y"] / 4 + 20) %>; left: 113.5" alt="Folder" src="<%- theme.preview.folder %>">
    <img width="74" height="74" style="position: absolute;top: <%- ((mainmenu["items_menu"]["y"] - 20) / 4 + 20) %>; left: 39.5" alt="Cursor" src="<%- theme.preview.cursor %>">
    <img width="64" height="64" style="position: absolute;top: <%- (mainmenu["items_menu"]["y"] / 4 + 20) %>; left: 182.5" alt="Folder" src="<%- theme.preview.folder %>">
    <img width="64" height="64" style="position: absolute;top: <%- (mainmenu["items_menu"]["y"] / 4 + 20) %>; left: 251.5" alt="Folder" src="<%- theme.preview.folder %>">
    <h4 style="position:absolute;top:190;left:1;right:1;font-family: 'OpenSans';"><%- theme.name %></h4>
    <h6 style="position:absolute;top:210;left:1;right:1;font-family: 'OpenSans';"><%- theme.description %></h6>
    <h5 style="position:absolute;top:230;left:1;right:1;font-family: 'OpenSans';"><%- options.lang.ulaunch.by %> <%- theme.author %></h5>
    <input style="position:absolute;top:275;left:55;right:55;font-family: 'OpenSans';" class="tdl" type="button" value="<%- options.lang.ulaunch.download %>" id="<%- num %>"/>
  </div>
</div>
